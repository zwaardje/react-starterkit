to: src/components/<%= h.inflection.pluralize(h.changeCase.paramCase(type)) %>/<%= name %>/__tests__/<%= name %>.spec.tsx
---
import * as React from 'react';
import * as renderer from 'react-test-renderer';

import <%= name %> from '../<%= name %>';

import {
    defaultProps,
} from '../__data__/<%= name %>.mockdata';

describe('<%= name %>', () => {

    it('renders default <%= h.changeCase.lower(name) %> correctly', () => {
        const props = { ... defaultProps };
        const tree = renderer.create(<<%= name %> {... props} />).toJSON();
        expect(tree).toMatchSnapshot();
    });

});
